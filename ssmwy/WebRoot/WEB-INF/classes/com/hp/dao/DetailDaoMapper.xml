<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hp.dao.DetailDao">
	<!-- 全查 -->
	<select id="queryAllde" resultType="Business">
		select * from t_detail;
	</select>
	<!-- 添加 -->
	<insert id="insertde">
		insert into t_detail(bId,bdName,bdPerson) values (#{bId},#{bdName},#{bdPerson})
	</insert>
	<!-- 删除 -->
	<delete id="deletede">
		delete * from t_detail where bdId=#{bdId}
	</delete>
	
</mapper>